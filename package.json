{
  "name": "hy-img",
  "version": "1.0.0-stencil.0",
  "description": "",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist www",
    "test": "exit 0",
    "build": "npm run build:tsc && npm run build:copy",
    "build:tsc": "tsc --outDir dist",
    "build:copy": "find src -name '*.js' -exec cp {} dist \\;",
    "watch": "npm run watch:tsc",
    "watch:tsc": "tsc -w --outDir dist",
    "build-docs": "stencil build --docs",
    "preversion": "sed -i '' 's:/dist:#/dist:' .gitignore",
    "version": "npm run build && git add .",
    "postversion": "sed -i '' 's:#/dist:/dist:' .gitignore && git rm --cached -r dist && git add . && git commit -m 'restore preversion .gitignore'"
  },
  "config": {
    "port": "8082"
  },
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
  "license": "GPL-3.0",
  "devDependencies": {
    "@stencil/core": "^0.18.1"
  },
  "dependencies": {
    "@skatejs/element": "0.0.1",
    "@skatejs/element-preact": "0.0.1",
    "preact": "^8.4.2",
    "rxjs": "^6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwtel/hy-img.git"
  },
  "bugs": {
    "url": "https://github.com/qwtel/hy-img/issues"
  },
  "homepage": "https://qwtel.com/hy-img/"
}
